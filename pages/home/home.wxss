.catelist{
  display: flex;
  justify-content: center;
}
.image{
  flex: 1;
  width: 190rpx;
 margin-top: 20rpx;
}
.red{
  color: red;
}
.blue{
  color: blue;
}
.yellow{
  color: yellow;
}
.floorimage{
  width: 100%;
}
.floor_list{
  overflow: hidden;
}
.floor_list navigator{
  float: left;
  width: 32%;
}
.floor_list navigator image{
  width: 100%;
  height: 100%;
}
.floor_list navigator:nth-last-child(-n+4){
  height: 196rpx;
}
.floor_list navigator:nth-child(2){
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(3){
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(4){
  border-left: 10rpx solid #fff;
  border-top: 10rpx solid #fff;
}
.floor_list navigator:nth-child(5){
  border-left: 10rpx solid #fff;
  border-top: 10rpx solid #fff;
}